{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "semi": 0,
    "max-len": 0,
    "consistent-return": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "object-curly-spacing": 0,
    "import/no-unresolved": [2, { ignore: ['electron'] }],
    "react/prefer-es6-class": 0,
    "react/prefer-stateless-function": 0,
  },
  "plugins": [
    "import",
    "react"
  ],
  "settings": {
    "import/resolver": "webpack"
  }
}
