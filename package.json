  "scripts": {
    "start": "npm run build-electron && npm run build-worker && electron dist/main.js",
    "build": "NODE_ENV=production sh -c 'rm -r ./dist && npm run build-renderer && npm run build-electron && npm run build-worker && cp package.json dist/ && cd dist/ && npm install --production'",
    "build-renderer": "babel-node ./node_modules/webpack/bin/webpack --config webpack.renderer.$NODE_ENV.js --colors",
    "build-electron": "babel-node ./node_modules/webpack/bin/webpack --config webpack.electron.$NODE_ENV.js --colors && cp src/renderer/index.html dist/",
    "build-worker": "babel-node ./node_modules/webpack/bin/webpack --config webpack.worker.js --colors",
    "webpack-dev-server": "babel-node node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.renderer.development.js --colors",
    "development": "NODE_ENV=development concurrently -k 'npm run webpack-dev-server' 'npm run start'",
    "production": "NODE_ENV=production sh -c 'npm run build-renderer && npm run start'"
  }
}
